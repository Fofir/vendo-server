  "dependencies": {
    "@goodwaygroup/lib-hapi-rollbar": "^0.6.2",
    "@hapi/boom": "^10.0.0",
    "@hapi/cookie": "^12.0.0",
    "@hapi/glue": "^8.0.0",
    "@hapi/hapi": "^20.2.2",
    "@hapi/inert": "^7.0.0",
    "@prisma/client": "^4.7.1",
    "@types/axios": "^0.14.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.29.1",
    "dotenv": "^16.0.1",
    "hapi-pino": "^10.1.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "8.5.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/hapi__cookie": "^12.0.0",
    "@types/hapi__hapi": "^20.0.12",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.3",
    "@types/node-notifier": "^8.0.2",
    "@types/uuid": "^8.3.4",
    "dotenv-cli": "^6.0.0",
    "jest": "^28.1.3",
    "nock": "^13.2.9",
    "prettier": "2.7.1",
    "prisma": "^4.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "migrate-and-start:dev": "npm run migrate:dev && npm start",
    "migrate-and-start:deploy": "npm run migrate:deploy && npm start",
    "build": "prisma generate && tsc",
    "start": "node dist/app/index.js",
    "compile": "tsc",
    "dev": "dotenv -e .env ts-node-dev -- --files ./app/index.ts",
    "db:push": "prisma db push",
    "migrate:reset": "prisma migrate reset",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "migrate:init": "npx prisma migrate dev --name init",
    "docker:test:up": "docker compose up -d --force-recreate",
    "docker:down": "docker compose down",
    "prisma:migrate:test": "dotenv -e .env.test -- prisma migrate reset --force",
    "setup-test": "npm run prisma:migrate:test",
    "test": "npm run setup-test && dotenv -e .env.test jest -- -i"
  }
}
